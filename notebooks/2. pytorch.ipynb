  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {},
   "outputs": [],
   "source": [
    "dataset_train = torch.utils.data.Subset(dataset, list(range(0, train_len)))\n",
    "dataset_val = torch.utils.data.Subset(dataset, list(range(train_len, len(dataset))))"
